    <div class="panel panel-default col-md-6 pull-right" id="panel-container">
      <div class="panel-heading">
        <h2 class="panel-title">Activity Leaderboard</h2>
      </div>
        
      <div class="container-fluid">
        
        <div class="row">
          <div class="col-xs-12">        
            <ul class="nav nav-tabs">
              <li role="presentation" class="active"><a href="#">Today</a></li>
              <li role="presentation"><a href="#">This Week</a></li>
              <li role="presentation"><a href="#">Last 30 Days</a></li>
            </ul>
          </div>
        </div>
        
        
        
        <div class="row">
          <div class="col-xs-6">
            User:  <%= current_user.first_name %> <%= current_user.last_name %>
          </div>
          <div class="col-xs-6">
            Signed in: <%= current_user.current_sign_in_at.strftime("%m/%d/%y at %I:%M %p") %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-6">
            Username: <%= current_user.username %>
          </div>
          <div class="col-xs-6">
            Last Sign in: <%= current_user.last_sign_in_at.strftime("%m/%d/%y at %I:%M %p") %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-6">
            User Signed up on: <%= current_user.created_at.strftime("%m/%d/%y") %>
          </div>
          <div class="col-xs-6">
            User Sign-ins: <%= current_user.sign_in_count %>
          </div>
        </div>

        <div class="row">
          <div class="col-xs-6">
            Record dispositions <%= @prospect.results.count %>
          </div>
          <div class="col-xs-6">
            Last disposition: <%= @prospect.results.last.disposition %>
          </div>
        </div>

        <div class="row">
          <div class="col-xs-12">
            Activity Leaderboard will be here - This is temporary placeholder data.
          </div>
        </div>
      </div>

        <div class="panel-footer">
          Footer
        </div>
      </div>